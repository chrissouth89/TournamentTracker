    <h1>Looking for Game!</h1>
    <form action="/teamgames" method="POST">
      <div class="form-group">
        <input type="text" name="name" placeholder='Team Name'/><br />
      </div>
      <div class="form-group">
        <label for="exampleSelect1">Select your game</label>
        <select class="form-control" id="exampleSelect1">
              <option>League of Legends</option>
              <option>Fortnite</option>
              <option>Call of Duty</option>
              <option>Borderlands</option>
              <option>Dota 2</option>
            </select>
          </div>
      </div>
      <input type="submit" name="" value="Looking for Game" />
    </form>
